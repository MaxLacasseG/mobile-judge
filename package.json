{
	"name": "jugement-mobile",
	"version": "1.0.0",
	"description": "Application pour le jugement des finales r√©gionales des Expo-sciences",
	"main": "server.js",
	"scripts": {
		"client-install": "npm install --prefix client",
		"start": "export NODE_ENV=develop && node server",
		"server": "nodemon app",
		"client": "npm start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\" ",
		"dev-debug": "concurrently \"npm --inspect run server\" \"npm --inspect run client\"",
		"heroku-postbuild": "export NODE_ENV=production && NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
	},
	"repository": {
		"type": "git",
		"url": "https://bitbucket.org/mlacassegermain-technoscience/jugement-mobile"
	},
	"author": "Maxime Lacasse Germain",
	"license": "MIT",
	"dependencies": {
		"bcrypt": "^3.0.2",
		"body-parser": "^1.18.3",
		"concurrently": "^4.1.0",
		"dotenv": "^6.2.0",
		"express": "^4.16.4",
		"jsonwebtoken": "^8.4.0",
		"mongoose": "^5.3.16",
		"morgan": "^1.9.1",
		"nodemailer": "^4.7.0",
		"passport": "^0.4.0",
		"passport-jwt": "^4.0.0",
		"prompt": "^1.0.0",
		"random-unique-id": "^1.1.0",
		"tracer": "^0.9.6",
		"uuid": "^3.3.2",
